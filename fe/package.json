{
  "name": "fe",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
    "format": "prettier --log-level=log --write \"**/*.{vue,js,jsx,cjs,mjs,css,md}\"",
    "prepare": "cd .. && husky fe/.husky"
  },
  "lint-staged": {
    "*.{ts,js,vue}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@wangeditor/editor": "^5.1.23",
    "@wangeditor/editor-for-vue": "^5.1.12",
    "axios": "^1.7.7",
    "element-plus": "^2.8.3",
    "pinia": "^2.2.2",
    "vue": "^3.5.6",
    "vue-icons-plus": "^0.1.6",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "husky": "^9.1.6",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.28.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.6",
    "vue-tsc": "^2.1.6"
  }
}
